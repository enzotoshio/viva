global-reset()

div, section, h1, h2, h3
  box-sizing: border-box

.header
  width: 100%
  height: 60px
  background-color: #1190cd
  overflow: hidden
  
.sidebar
  width: 107px
  height: 660px
  background-color: #094e6e
  
.logo-container
  width: 107px
  height: 60px
  background-color: #00acff
  overflow: hidden
  
.logo-vivareal
  width: 48px
  height: 38px
  object-fit: contain
  background-color: #ffffff
  display: block
  text-indent: -9999px
  background: url(../images/logo-vivareal.svg)
  background-size: 48px 38px
  background-repeat: no-repeat
  margin-left: 30px
  margin-top: 11px